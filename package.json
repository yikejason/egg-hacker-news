  },
  "devDependencies": {
    "@eggjs/tsconfig": "^1.1.0",
    "@types/cheerio": "^0.22.1",
    "@types/mocha": "^10.0.1",
    "cheerio": "^1.0.0-rc.2",
    "egg-bin": "6",
    "egg-mock": "5",
    "eslint": "9.23.0",
    "eslint-config-egg": "14.1.0",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
